{
  "manifest_version": 2,

  "name": "Tabtree",
  "description": "Tab tree.",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "tree-icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://secure.flickr.com/",
    "tabs"
  ]
}
