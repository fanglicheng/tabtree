{
  "manifest_version": 2,
   "background": {
     "persistent": true,
     "scripts": ["background.js"]
   },
   "browser_action": {
      "default_icon": "pacman32.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "js": [ "inject.js" ],
      "matches": [ "\u003Call_urls\u003E" ]
   } ],
   "description": "Never close another tab by yourself.",
   "icons": {
      "128": "pacman128.png",
      "16": "pacman16.png",
      "48": "pacman48.png"
   },
   "name": "AutoTabs",
   "permissions": [ "tabs" ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "1.2"
}
