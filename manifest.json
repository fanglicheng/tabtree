{
  "manifest_version": 2,

  "name": "Tabtree",
  "description": "Sort your tabs and show them as a tree.",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "tree-icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
