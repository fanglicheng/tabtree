{
  "manifest_version": 2,

  "name": "Tabtree",
  "description": "Sort and group your tabs as a tree.",
  "version": "0.1",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "tree-icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
